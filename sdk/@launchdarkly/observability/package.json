{
	"name": "@launchdarkly/observability",
	"version": "0.4.6",
	"description": "Browser observability for your web app. Capture frontend metrics, errors, logs and traces.",
	"keywords": [
		"launchdarkly",
		"error monitoring",
		"logging",
		"tracing",
		"metrics",
		"debugging",
		"observability",
		"browser",
		"library"
	],
	"homepage": "https://github.com/launchdarkly/observability-sdk#readme",
	"bugs": {
		"url": "https://github.com/launchdarkly/observability-sdk/issues",
		"email": "support@launchdarkly.com"
	},
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/launchdarkly/observability-sdk.git"
	},
	"scripts": {
		"typegen": "tsc",
		"build": "yarn build:minimal && yarn build:full",
		"build:minimal": "vite build --config vite.config.minimal.ts",
		"build:full": "vite build",
		"size": "size-limit"
	},
	"dependencies": {
		"highlight.run": "workspace:*"
	},
	"devDependencies": {
		"rollup-plugin-visualizer": "^5.14.0",
		"typescript": "^5.8.3",
		"vite": "^6.3.4",
		"vitest": "^3.1.2"
	},
	"type": "module",
	"main": "./dist/index-minimal.js",
	"module": "./dist/index-minimal.js",
	"types": "./dist/index-minimal.d.ts",
	"exports": {
		".": {
			"import": "./dist/index-minimal.js",
			"require": "./dist/index-minimal.cjs",
			"types": "./dist/index-minimal.d.ts"
		},
		"./full": {
			"import": "./dist/observability.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"size-limit": [
		{
			"name": "Minimal Build",
			"path": "dist/index-minimal.js",
			"limit": "5 kB",
			"brotli": true
		},
		{
			"name": "Full Build",
			"path": "dist/observability.js",
			"limit": "100 kB",
			"brotli": true
		}
	]
}
